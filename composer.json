{
  "name": "visma/assignment",
  "description": "Visma staff register assignment.",
  "license": "MIT",
  "type": "project",
  "require": {
    "doctrine/orm": "^2.6",
    "league/csv": "^9.1",
    "respect/validation": "^1.1",
    "symfony/var-dumper": "^4.0",
    "fzaninotto/faker": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "prefer-stable": true
}